hto:HeattreatmentScope rdf:type owl:Class ;
                       rdfs:subClassOf pmdco:HeatTreatmentProcess ;
                       rdfs:isDefinedBy <https://w3id.org/pmd/hto/> ;
                       skos:definition "This class specifices the scope of the specific heattreatment process."@en ;
